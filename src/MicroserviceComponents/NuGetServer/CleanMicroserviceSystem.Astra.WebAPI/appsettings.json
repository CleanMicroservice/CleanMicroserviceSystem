{
    "urls": "https://*:11003",

    "JwtBearerConfiguration": {
        "JwtSecurityKey": "RANDOM_KEY_MUST_NOT_BE_SHARED",
        "JwtIssuer": "Themis",
        "JwtAudience": "Tethys",
        "JwtExpiryForUser": 30,
        "JwtExpiryForClient": 1440
    },

    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },

    "NuGetServerConfiguration": {
        "PackagePath": "D:\\NuGetPackages",
        "ApiKey": "ASTRANUGETSERVERAPIKEY"
    },

    "AgentServiceRegistrationConfiguration": {
        "ServicesProviderAddress": "http://localhost:8500",
        "ServiceName": "NuGetServer-Astra",
        "ServiceInstanceId": "NuGetServer_Astra_Dev_000",
        "SelfHost": "localhost",
        "SelfPort": "11003",
        "HealthCheckUrl": "https://localhost:11003/healthcheck"
    },

    "ConnectionStrings": {
        "ServiceDB": "DATA SOURCE=.\\Service.db"
    },

    "AllowedHosts": "*"
}
