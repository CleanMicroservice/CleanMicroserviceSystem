@page "/system/notfound"
@attribute [AllowAnonymous]
@inject NavigationManager navigationManager

<PageTitle>Not Found</PageTitle>

<MContainer Class="mt-10">
    <MRow Justify="@JustifyTypes.Center"
          NoGutters>
        <MSpacer></MSpacer>
        <MCol Cols="@("auto")">
            <MImage Src="/img/status/404.png" Width="350" Height="200"></MImage>
            <MButton Outlined Block Color="primary" OnClick="()=>navigationManager.NavigateTo(RouterContract.DefaultUri)">Home</MButton>
        </MCol>
        <MSpacer></MSpacer>
    </MRow>
</MContainer>

